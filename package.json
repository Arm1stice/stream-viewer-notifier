{
  "build": {
    "appId": "com.arm1stice.twitch-viewer-notifier",
    "productName": "Twitch Viewer Notifier",
    "electronCompile": true,
    "win": {
      "target": "squirrel",
      "certificateSubjectName": "Wyatt Calandro"
    }
  },
  "devDependencies": {
    "electron": "^3.0.3",
    "electron-builder": "^20.28.4",
    "electron-compilers": "^5.9.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "electron-compile": "^6.4.3",
    "electron-log": "^2.2.17",
    "electron-settings": "^3.2.0",
    "electron-updater": "^3.1.2",
    "escape-html": "^1.0.3",
    "jquery": "^3.3.1",
    "keyboardjs": "^2.4.1",
    "nedb": "^1.8.0"
  },
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dev": "electron-builder --dir && dist\\win-unpacked\\\"Twitch Viewer Notifier.exe\"",
    "run": "dist\\win-unpacked\\\"Twitch Viewer Notifier.exe\"",
    "pub": "electron-builder --publish always"
  },
  "name": "twitch-viewer-notifier",
  "version": "0.0.9",
  "description": "Twitch Viewer Notifier",
  "main": "./src/index.ts",
  "repository": "https://github.com/wcalandro/twitch-viewer-notifier.git",
  "author": "Wyatt Calandro <wyatt@wcalandro.com>",
  "license": "GPL-3.0-only",
  "private": false
}
